generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @default(autoincrement()) @id
  userName String @unique
  displayName String? @default("")
}

model UserSettings {
  id Int @default(autoincrement()) @id
  notificationsOn Boolean
}

model Post {
  id Int @default(autoincrement()) @id
  title String
  description String
}





